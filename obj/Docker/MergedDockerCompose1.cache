services:
  ravendb:
    container_name: roomservicemanagementravendb
    environment:
      RAVEN_ARGS: '"--Setup.Mode=None"'
      RAVEN_Security_UnsecuredAccessAllowed: PublicNetwork
    image: ravendb/ravendb
    ports:
    - published: 8080
      target: 8080
  roomservicemanagement.api:
    build:
      context: /Users/jorgeoliverodriguez/Projects/UOCHotels.RoomServiceManagement
      dockerfile: RoomServiceManagement.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: RoomServiceManagement.Api
      target: base
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44386'
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: roomservicemanagement:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "bin/Debug/netcoreapp2.2/RoomServiceManagement.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 53838
      target: 80
    - published: 44386
      target: 443
    volumes:
    - /Users/jorgeoliverodriguez/Projects/UOCHotels.RoomServiceManagement/RoomServiceManagement.Api:/app:rw
    - /Users/jorgeoliverodriguez/vsdbg/vs2017u5:/remote_debugger:ro
    - /Users/jorgeoliverodriguez/.aspnet/https:/root/.aspnet/https:ro
    - /Users/jorgeoliverodriguez/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /usr/local/share/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /Users/jorgeoliverodriguez/.nuget/packages:/root/.nuget/packages:ro
version: '3.4'